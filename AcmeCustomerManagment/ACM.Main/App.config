<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <add key="ConnectionString" value="Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=ACM;Integrated Security=True;Connect Timeout=15;Encrypt=False;TrustServerCertificate=True;ApplicationIntent=ReadWrite;MultiSubnetFailover=False" />
    <add key="CustomerTable" value="Customer"/>
    <add key="CustomerTypeTable" value="CustomerType"/>
    <add key="CustomerCommand" value="SELECT * FROM Customer"/>
    <add key="CustomerTypeCommand" value="SELECT * FROM CustomerType"/>
  </appSettings>
</configuration>